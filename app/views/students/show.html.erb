<h1>Welcome <%= @student.name.capitalize %></h1>

<div class='taken'>
  <ol>
    <% @student.quizzes.each do |q| %>
    <li><a href="/instructors_quizzes/<%= q.id %>"><%= q.name %></a></li>
    <% end %>
  </ol>
</div>


<%= button_to 'Check for New Quiz', students_quizzes_path, method: :get %>
<%= button_to 'Logout', logout_path, method: :delete %>
